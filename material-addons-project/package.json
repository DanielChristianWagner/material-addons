{
  "name": "material-addons-project",
  "version": "0.0.0",
  "scripts": {
    "build:mat-add": "ng build material-addons && npm run build:themes",
    "build:themes": "scss-bundle",
    "patch:mat-add": "cd projects/material-addons && npm version patch",
    "pack:mat-add": "npm pack ./dist/material-addons",
    "publish:mat-add": "npm run patch:mat-add && npm run build:mat-add && npm publish ./dist/material-addons --access public",
    "deploy:demo": "ng build --prod --base-href https://porscheinformatik.github.io/material-addons/ && npx angular-cli-ghpages --dir=dist/material-addons-project"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/cdk": "^8.2.2",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.0",
    "@angular/material": "^8.2.2",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.1",
    "@angular-devkit/build-ng-packagr": "~0.802.2",
    "@angular/cli": "~8.2.1",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "ng-packagr": "^5.3.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.36.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "scss-bundle": "^3.0.1"
  },
  "optionalDependencies": {
    "@porscheinformatik/material-addons": "file:dist/material-addons"
  }
}
